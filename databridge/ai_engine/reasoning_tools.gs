function AI_titleScore(t){t=String(t||'');return Math.min(100,40+(t.length>44?25:10)+(/\bhow\b|\bguide\b|\breview\b/i.test(t)?15:0));}function AI_outlineDepthScore(o){return Array.isArray(o)?Math.min(100,50+o.length*7):62;}function AI_semanticDensityRatio(text){text=String(text||'');var tokens=text.split(/\s+/).length||1;var longs=(text.match(/\b[a-z]{8,}\b/ig)||[]).length;return longs/tokens;}
